#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/steipete/Projects/clawdbot4/node_modules/.pnpm/clawdbot@2026.1.23-1_@types+express@5.0.6_audio-decode@2.2.3_devtools-protocol@0.0.1561482_typescript@5.9.3/node_modules/clawdbot/dist/node_modules:/Users/steipete/Projects/clawdbot4/node_modules/.pnpm/clawdbot@2026.1.23-1_@types+express@5.0.6_audio-decode@2.2.3_devtools-protocol@0.0.1561482_typescript@5.9.3/node_modules/clawdbot/node_modules:/Users/steipete/Projects/clawdbot4/node_modules/.pnpm/clawdbot@2026.1.23-1_@types+express@5.0.6_audio-decode@2.2.3_devtools-protocol@0.0.1561482_typescript@5.9.3/node_modules:/Users/steipete/Projects/clawdbot4/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/steipete/Projects/clawdbot4/node_modules/.pnpm/clawdbot@2026.1.23-1_@types+express@5.0.6_audio-decode@2.2.3_devtools-protocol@0.0.1561482_typescript@5.9.3/node_modules/clawdbot/dist/node_modules:/Users/steipete/Projects/clawdbot4/node_modules/.pnpm/clawdbot@2026.1.23-1_@types+express@5.0.6_audio-decode@2.2.3_devtools-protocol@0.0.1561482_typescript@5.9.3/node_modules/clawdbot/node_modules:/Users/steipete/Projects/clawdbot4/node_modules/.pnpm/clawdbot@2026.1.23-1_@types+express@5.0.6_audio-decode@2.2.3_devtools-protocol@0.0.1561482_typescript@5.9.3/node_modules:/Users/steipete/Projects/clawdbot4/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../clawdbot/dist/entry.js" "$@"
else
  exec node  "$basedir/../clawdbot/dist/entry.js" "$@"
fi
